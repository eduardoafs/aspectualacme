    <eLiterals name="before"/>
    <eLiterals name="around" value="1"/>
    <eLiterals name="after" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Armani">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="tokens" upperBound="-1"
